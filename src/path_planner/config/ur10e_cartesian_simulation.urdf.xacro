<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ur10e">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <xacro:include filename="$(find ur_description)/urdf/ur10e.urdf.xacro" />

    <xacro:include filename="$(find my_moveit_config)/config/ur10e.ros2_control.xacro" />

    <xacro:ur10e_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>